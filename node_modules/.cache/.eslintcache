rc\\components\\Products.jsx",["106"],"import { useEffect, useState } from \"react\"\r\nimport styled from \"styled-components\"\r\nimport { popularProducts } from \"../data\"\r\nimport Product from \"./Product\"\r\n//import axios from \"axios\"\r\nimport { publicRequest } from \"../requestMethods\"\r\n\r\nconst Container = styled.div`\r\n padding: 20px;\r\n display: flex;\r\n flex-wrap:wrap;\r\n justify-content: space-between;`\r\n\r\nconst Products = ({cat,filters,sort}) => {\r\n\r\n    const [products,setProducts] = useState([]);\r\n    const [filteredProducts,setFilteredProducts] = useState([]);\r\n\r\n    useEffect(()=>{\r\n        const getProducts = async ()=>{\r\n            try{\r\n                const res = await publicRequest.get(\r\n                   cat \r\n                     ? `/products?category=${cat}` \r\n                     : \"/api/products\"\r\n                );\r\n                setProducts(res.data);\r\n            }catch(err){}\r\n        };\r\n\r\n        getProducts();\r\n    },[cat])\r\n\r\n    useEffect(() => {\r\n        cat && \r\n            setFilteredProducts(\r\n                products.filter(item => \r\n                    Object.entries(filters).every(([key,value])=>\r\n                        item[key].includes(value)\r\n                        )\r\n                    )\r\n            );\r\n    }, [products,cat,filters])\r\n\r\n    useEffect(()=>{\r\n        if(sort === \"newest\"){\r\n            setFilteredProducts((prev)=>\r\n                [...prev].sort((a,b)=>a.createdAt - b.createdAt)\r\n                );\r\n        } else if(sort === \"asc\"){\r\n            setFilteredProducts((prev)=>\r\n                [...prev].sort((a,b)=>a.price - b.price)\r\n                );\r\n        } else {\r\n            setFilteredProducts((prev)=>\r\n                [...prev].sort((a,b)=>b.price - a.price)\r\n                );\r\n        }\r\n    },[sort])\r\n\r\n    return (\r\n        <Container>\r\n            {cat ? filteredProducts.map((item) => (\r\n                <Product item={item} key={item.id} />\r\n                )) : products.slice(0,8).map((item) => (\r\n                    <Product item={item} key={item.id} />\r\n                    ))}\r\n        </Container>\r\n    )\r\n}\r\n\r\nexport default Products\r\n","C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\components\\Newsletter.jsx",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\components\\Slider.jsx",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\data.js",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\components\\CategoryItem.jsx",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\components\\Product.jsx",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\requestMethods.js",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\redux\\store.js",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\redux\\cartRedux.js",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\pages\\Success.jsx",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\redux\\userRedux.js",[],"C:\\Users\\danie\\Documents\\Programming\\React\\GitHub\\Proyects\\Ecoomerce\\SHOP\\client\\src\\redux\\apiCalls.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":3,"column":10,"nodeType":"113","messageId":"114","endLine":3,"endColumn":25},"no-native-reassign",["115"],"no-negated-in-lhs",["116"],"no-unused-vars","'popularProducts' is defined but never used.","Identifier","unusedVar","no-